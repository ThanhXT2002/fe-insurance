{
  "name": "fe-insurance",
  "author": "tran_xuan_thanh",
  "email": "tranxuanthanhtxt2002@gmail.com",
  "version": "0.0.1",
  "scripts": {
     "ng": "ng",
    "start": "ng serve --port=4600 --host=0.0.0.0",
    "dev": "ng serve --port=4600 --host=0.0.0.0",
    "build": "ng build",
    "build:ssr": "ng build --configuration=production",
    "build:ssr:watch": "ng build --configuration=production --watch",
    "serve:ssr": "node dist/fe-insurance/server/server.mjs",
    "serve:ssr:watch": "nodemon dist/fe-insurance/server/server.mjs",
    "dev:ssr": "npm run build:ssr && npm run serve:ssr",
    "dev:ssr:watch": "concurrently --names \"BUILD,SERVE\" --prefix-colors \"yellow,green\" \"npm run build:ssr:watch\" \"npm run serve:ssr:watch\"",
    "dev:both": "concurrently --names \"SPA,SSR\" --prefix-colors \"blue,green\" \"npm run dev\" \"npm run dev:ssr\"",
    "dev:all": "concurrently --names \"SPA,SSR-BUILD,SSR-SERVE\" --prefix-colors \"blue,yellow,green\" \"npm run dev\" \"npm run build:ssr:watch\" \"npm run serve:ssr:watch\"",
    "prerender": "ng run fe-insurance:prerender",
    "prod": "npm run build:ssr && npm run serve:ssr",
    "test": "ng test",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write ."
  },
  "prettier": {
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "20.1.4",
    "@angular/cdk": "20.1.4",
    "@angular/common": "20.1.0",
    "@angular/compiler": "20.1.0",
    "@angular/core": "20.1.0",
    "@angular/forms": "20.1.0",
    "@angular/platform-browser": "20.1.0",
    "@angular/platform-server": "20.1.4",
    "@angular/router": "20.1.0",
    "@angular/service-worker": "20.1.0",
    "@angular/ssr": "20.1.4",
    "@fontsource/open-sans": "5.2.6",
    "@fontsource/pt-serif": "5.2.6",
    "@fortawesome/fontawesome-free": "7.0.0",
    "@ngrx/signals": "20.0.0",
    "@primeuix/themes": "1.2.2",
    "@tailwindcss/postcss": "4.1.11",
    "express": "5.1.0",
    "primeicons": "7.0.0",
    "primeng": "20.0.1",
    "remixicon": "4.6.0",
    "rxjs": "7.8.0",
    "swiper": "11.2.10",
    "tailwind-scrollbar": "3.1.0",
    "tailwind-scrollbar-hide": "1.3.1",
    "tailwindcss-primeui": "0.6.1",
    "tslib": "2.3.0",
    "zone.js": "0.15.1"
  },
  "devDependencies": {
    "@angular/build": "20.1.4",
    "@angular/cli": "20.1.4",
    "@angular/compiler-cli": "20.1.0",
    "@types/express": "5.0.1",
    "@types/jasmine": "5.1.0",
    "@types/node": "20.17.19",
    "autoprefixer": "10.4.21",
    "concurrently": "9.2.0",
    "http-server": "14.1.1",
    "jasmine-core": "5.8.0",
    "karma": "6.4.0",
    "karma-chrome-launcher": "3.2.0",
    "karma-coverage": "2.2.0",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.1.0",
    "nodemon": "3.1.10",
    "postcss": "8.5.6",
    "prettier": "3.6.2",
    "tailwindcss": "3.4.17",
    "typescript": "5.8.2"
  }
}
